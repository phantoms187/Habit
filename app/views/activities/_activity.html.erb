<li id="activity-<%= activity.id %>">
  <span class="user"><%= link_to activity.user.name, activity.user %></span>
  <span class="content" style="color:#eaeaea;">
    
    Climb: <%= activity.climb %> 
    Duolingo: <%= activity.duolingo %> 
    Teeth: <%= activity.teeth %> 
    Mia: <%= activity.mia %> 
    Run: <%= activity.run %> 
    Keyboard: <%= activity.keyboard %> 
    Pull: <%= activity.pull %> 
    Floor: <%= activity.floor %> 
    Bell: <%= activity.bell %> 

  </span>
  <span class="timestamp">
    Posted <%= time_ago_in_words(activity.created_at) %> ago.
    <% if current_user?(activity.user) %>
      <%= link_to "delete", activity, method: :delete,
                                       data: { confirm: "You sure?" } %>
    <% end %>
  </span>
</li>